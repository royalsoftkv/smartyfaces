{
  "name": "royalsoft/smartyfaces-framework",
  "description": "PHP Ajax Framework based on Smarty",
  "authors": [
    {
      "name": "RoyalSoft",
      "email": "royalsoftkv@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
